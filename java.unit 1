
import java.util.*;

public class Main
{
	public static void main(String[] args) {
	    	Scanner sc=new Scanner(System.in);
	int size;
	int marks[]=new int[5];
	for(int i=0;i<5;i++){
	    System.out.println("enter the elements in the array");
	    marks[i]=sc.nextInt();
	}
	for(int j=0;j<5;j++){
	    System.out.println(marks[j]);
	
	}
	
	}
}
